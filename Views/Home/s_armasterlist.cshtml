s_armaster_List.RowType = Config.RowTypeAggregateInit;
s_armaster_List.ResetAttributes();
await s_armaster_List.RenderRow();

// Output data rows
bool eof = (s_armaster_List.Recordset == null); // DN
while (s_armaster_List.RecordCount < s_armaster_List.StopRecord) { // Begin While
	if (!s_armaster_List.IsGridAdd && !eof) { // DN
		eof = !await s_armaster_List.Recordset.ReadAsync();
	}
	s_armaster_List.RecordCount++;
	if (s_armaster_List.RecordCount >= s_armaster_List.StartRecord) {
		s_armaster_List.RowCnt++;

		// Set up key count
		s_armaster_List.KeyCount = ConvertToInt(s_armaster_List.RowIndex);

		// Init row class and style
		s_armaster_List.ResetAttributes();
		s_armaster_List.CssClass = "";
		if (s_armaster_List.IsGridAdd) {
