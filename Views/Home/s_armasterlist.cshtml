} else {

	// Set the last record to display
	if (s_armaster_List.TotalRecords > s_armaster_List.StartRecord + s_armaster_List.DisplayRecords - 1) {
		s_armaster_List.StopRecord = s_armaster_List.StartRecord + s_armaster_List.DisplayRecords - 1;
	} else {
		s_armaster_List.StopRecord = s_armaster_List.TotalRecords;
	}
}
if (s_armaster_List.Recordset != null && s_armaster_List.Recordset.HasRows) {
	if (!s_armaster_List.Connection.SelectOffset) { // DN
		for (var i = 1; i <= s_armaster_List.StartRecord - 1; i++) { // Move to first record
			if (await s_armaster_List.Recordset.ReadAsync()) {
				s_armaster_List.RecordCount++;
			}
		}
	} else {
		s_armaster_List.RecordCount = s_armaster_List.StartRecord - 1;
	}
} else if (!s_armaster_List.AllowAddDeleteRow && s_armaster_List.StopRecord == 0) {
	s_armaster_List.StopRecord = s_armaster_List.GridAddRowCount;
}

